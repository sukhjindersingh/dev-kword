{
   "background": {
      "scripts": [ "js/background.js" ]
   },
   "content_scripts": [ {
      "all_frames": false,
      "js": [ "js/jq17.js", "js/tools.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_end"
   } ],
   "description": "TB ex tools",
   "icons": {
      "128": "icons/icon_128.png",
      "16": "icons/icon_016.png",
      "48": "icons/icon_048.png"
   },
   "manifest_version": 2,
   "minimum_chrome_version": "18",
   "name": "TB ex",
   "permissions": [ "webRequest", "tabs","*://*/*" ],
   "options_page": "options.html",
   "browser_action": {
		"default_icon":"icons/icon_016.png", 
		"default_popup": "popup.html",
		"default_title":"TB ex"
   },
   "version": "1.0"
}
